<schemacrawler-linter-configs>
  <linter id="linter.Linter1">
    <severity>medium</severity>
    <config>
      <property name="exclude">.*</property>
      <property name="other">something else</property>
    </config>
  </linter>
  <linter id="linter.Linter2">
    <run>false</run>
    <config>
      <property name="exclude">.*</property>
    </config>
  </linter>
  <linter id="linter.Linter3">
    <severity>high</severity>
    <run>true</run>
  </linter>
</schemacrawler-linter-configs>