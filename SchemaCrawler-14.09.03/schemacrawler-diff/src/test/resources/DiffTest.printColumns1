schema: 
  table: Authors
      column: Authors.Id
      column: Authors.FirstName
      column: Authors.LastName
      column: Authors.Address1
      column: Authors.Address2
      column: Authors.City
      column: Authors.State
      column: Authors.PostalCode
      column: Authors.Country
  table: BookAuthors
      column: BookAuthors.BookId
      column: BookAuthors.AuthorId
      column: BookAuthors."UPDATE"
  table: Books
      column: Books.Id
      column: Books.Title
      column: Books.Description
      column: Books.PublisherId
      column: Books.PublicationDate
      column: Books.Price
      column: Books.PreviousEditionId
